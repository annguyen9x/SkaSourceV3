	 "ChiTietHoaDon"."SoHD" = "HoaDon"."SoHD" 
	 AND "HoaDon"."SoHD" = $P{soHD}) 
	INNER JOIN "NguoiNhanHang" ON 
	  ( 
	 "HoaDon"."IDNN" = "NguoiNhanHang"."IDNN" 
